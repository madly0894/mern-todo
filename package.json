    "config": "^3.3.3",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "express-validator": "^6.7.0",
    "mongoose": "^5.11.4"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6"
  }
}
